{
  "name": "themerec",
  "displayName": "themeRec",
  "description": "Recommend A Random Theme",
  "version": "0.0.13",
  "publisher": "jtan",
  "icon": "clothing.png",
  "author": {
    "name": "Jialiang Tan",
    "email": "jtan02@wm.edu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JialiangTan/themerec.git"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:themerec.randTheme",
    "onCommand:themerec.defaultTheme",
    "onCommand:themerec.likeTheme",
    "onCommand:themerec.randLike",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "themerec.randTheme",
        "title": "Random Theme",
        "category": "Themerec"
      },
      {
        "command": "themerec.defaultTheme",
        "title": "Default Theme",
        "category": "Themerec"
      },
      {
        "command": "themerec.likeTheme",
        "title": "Like Theme",
        "category": "Themerec"
      },
      {
        "command": "themerec.randLike",
        "title": "Random in Like",
        "category": "Themerec"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
